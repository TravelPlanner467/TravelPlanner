    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/react-table": "^8.21.3",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "@vis.gl/react-google-maps": "^1.7.1",
    "better-auth": "^1.3.34",
    "concurrently": "^9.2.1",
