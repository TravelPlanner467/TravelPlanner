{
  "name": "travel-planner",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "flask-dev": "cross-env FLASK_DEBUG=1 NODE_ENV=development pip3 install -r requirements.txt && python3 -m flask --app api/py_index run -p 5328",
    "next-dev": "cross-env NODE_ENV=development next dev --turbopack",
    "dev": "concurrently \"pnpm run next-dev\" \"pnpm run flask-dev\"",
    "build": "next build",
    "start": "concurrently \"next start\" \"pnpm run flask-prod\"",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@heroicons/react": "^2.2.0",
    "@mui/material": "^7.3.5",
    "@prisma/client": "6.18.0",
    "@react-google-maps/api": "^2.20.7",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/postcss": "^4.1.17",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.3.34",
    "concurrently": "^9.2.1",
    "leaflet": "^1.9.4",
    "next": "latest",
    "prisma": "^6.18.0",
    "react": "latest",
    "react-dom": "latest",
    "react-leaflet": "^5.0.0",
    "resend": "^6.4.2",
    "tailwindcss": "4.1.17",
    "typescript": "5.7.3"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.21",
    "@types/node": "24.10.0",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "cross-env": "^10.1.0"
  }
}
